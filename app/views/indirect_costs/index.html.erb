<div class="page-header">
  <h1>
    Indirect costs
    <a href="/indirect_costs/new" class="btn btn-lg btn-success">New Indirect cost</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Project</th>
        <th>Profit</th>
        <th>Sga</th>
        <th>Rd</th>
        <th>Actions</th>
      </tr>

      <% @indirect_costs.each do |indirect_cost| %>
      <tr>
        <td><%= indirect_cost.project_id %></td>
        <td><%= indirect_cost.profit %></td>
        <td><%= indirect_cost.sga %></td>
        <td><%= indirect_cost.rd %></td>
        <td>
          <a href="/indirect_costs/<%= indirect_cost.id %>" class="btn btn-primary">Show</a>
          <a href="/indirect_costs/<%= indirect_cost.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_indirect_cost/<%= indirect_cost.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
